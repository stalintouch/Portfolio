<div class="container-fluid text-center">
  <%= video_tag "", autoplay:true, id: "video-background" %>
  <div class="row">
      <% @projects.each do |project| %>
        <div class="col-sm-6 col-md-3">
          <h2><a href="#"><%= link_to project.title, project, class: "title" %></a></h2>
          <%= link_to (image_tag project.image.url(:medium), class: "image img-rounded"), project %>
        </div>
      <% end %>
  </div>
</div>


